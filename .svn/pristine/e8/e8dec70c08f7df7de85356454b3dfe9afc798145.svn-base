package com.sitemap.na2ne.models;

/**
 * com.sitemap.na2ne.models.RefundListItemModel
 * 退款记录的每一列
 * @author zhang
 * create at 2015年12月28日 下午3:11:53
 */
public class RefundListItemModel {
	private String requestTime;//申请退款时间
	private int status;//退款处理状态 0：处理中 ，1：成功，2失败
	private double refundMoney;//退款金额
	private String failReason;//退款原因
	public String getRequestTime() {
		return requestTime;
	}
	public void setRequestTime(String requestTime) {
		this.requestTime = requestTime;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public double getRefundMoney() {
		return refundMoney;
	}
	public void setRefundMoney(double refundMoney) {
		this.refundMoney = refundMoney;
	}
	public String getFailReason() {
		return failReason;
	}
	public void setFailReason(String failReason) {
		this.failReason = failReason;
	}
	
}
