<?xml version="1.0" encoding="utf-8"?>
<!-- 追踪对话框 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:layout_margin="10dp"
    android:padding="5dp"
    android:gravity="center_horizontal"
    android:background="@color/index_tm"
     >
   
    <LinearLayout android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginRight="10dp"
        >
   	<LinearLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    
   	    >
   	    <TextView 
   	        android:layout_width="wrap_content"
   	        android:layout_height="wrap_content"
   	        android:text="@string/index_popup_zhuizong_locationtime"
   	       android:textColor="#333333"
                android:textSize="@dimen/_30px_in720p"
   	        android:layout_marginRight="10dp"
   	        />

   	    <TextView
   	        android:id="@+id/jiangesp"
   	        android:layout_width="180dp"
   	        android:layout_height="30dp"
   	        android:background="@drawable/index_border_shape"
   	        android:padding="5dp"
   	        android:textColor="#333333"
                android:textSize="@dimen/_30px_in720p"/>
   	    
   	</LinearLayout>
   	
   	 	<LinearLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginTop="10dp"
   	    >
   	    <TextView 
   	        android:layout_width="wrap_content"
   	        android:layout_height="wrap_content"
   	        android:text="@string/index_popup_zhuizong_endtime"
   	        android:textColor="#333333"
                android:textSize="@dimen/_30px_in720p"
   	        android:layout_marginRight="10dp"
   	        />
   	    <TextView 
   	        android:id="@+id/endjiangesp"
   	        android:layout_width="180dp"
   	        android:layout_height="30dp"
   	        android:background="@drawable/index_border_shape"
   	        android:padding="5dp"
   	       android:textColor="#333333"
                android:textSize="@dimen/_30px_in720p"
   	        />
   	    
   	</LinearLayout>
   </LinearLayout>
    
    <ImageView 
        android:id="@+id/zhuizong"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/index_start_zhuizong"
        android:layout_gravity="center_vertical"
        />

</LinearLayout>
