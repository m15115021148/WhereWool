   	        android:textSize="@dimen/_30px_in720p"
        android:textColor="#333333"
   	        android:layout_marginRight="10dp"
   	        />

   	    <EditText
   	        android:id="@+id/startDate"
   	        android:layout_width="180dp"
   	        android:layout_height="30dp"
   	        android:background="@drawable/index_border_shape"
   	        android:padding="5dp"
   	        android:hint="@string/index_popup_defuat_startdate"
   	        android:textSize="@dimen/_30px_in720p"
        android:textColor="#333333" />
   	    
   	</LinearLayout>
   	
   	 	<LinearLayout 
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_marginTop="10dp"
   	    >
   	    <TextView 
   	        android:layout_width="wrap_content"
   	        android:layout_height="wrap_content"
   	        android:text="@string/index_popup_guiji_enddate"
   	        android:textSize="@dimen/_30px_in720p"
        android:textColor="#333333"
   	        android:layout_marginRight="10dp"
   	        />
   	    <EditText 
   	        android:id="@+id/endDate"
   	        android:layout_width="180dp"
   	        android:layout_height="30dp"
   	        android:background="@drawable/index_border_shape"
   	        android:padding="5dp"
   	        android:hint="@string/index_popup_defuat_enddate"
   	       android:textSize="@dimen/_30px_in720p"
        android:textColor="#333333"
   	        />
   	    
   	</LinearLayout>
