    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:layout_margin="@dimen/_30px_in720p"
        android:orientation="horizontal" >

        <EditText
            android:id="@+id/register_phone_code_ed"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:layout_weight="1"
            android:background="@drawable/login_shape"
            android:hint="@string/register_phone_code"
            android:padding="@dimen/_15px_in720p"
            android:textColor="#333333"
            android:inputType="number"
            android:textColorHint="#dedede"
            android:textSize="@dimen/_30px_in720p"
            />

        <TextView
            android:id="@+id/send_code"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80px_in720p"
            android:layout_marginLeft="@dimen/_40px_in720p"
            android:layout_weight="1"
            android:background="@drawable/register_code"
            android:gravity="center"
            android:text="@string/register_phone_sendcode"
            android:textSize="@dimen/_30px_in720p"
            android:textColor="#ffffff" />
    </LinearLayout>
