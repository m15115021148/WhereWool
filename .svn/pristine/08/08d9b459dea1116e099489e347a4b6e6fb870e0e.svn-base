<?xml version="1.0" encoding="utf-8"?>
<!-- 手机注册页面 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/lan"
        android:padding="@dimen/_15px_in720p" >

        <LinearLayout
            android:id="@+id/base_back_lay"
            android:layout_width="@dimen/_80px_in720p"
            android:layout_height="@dimen/_80px_in720p"
            android:layout_centerVertical="true"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/back_tv"
                android:layout_width="@dimen/_20px_in720p"
                android:layout_height="@dimen/_30px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:background="@drawable/back_index" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/register_phone_title"
            android:textColor="@color/white"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:layout_margin="@dimen/_30px_in720p"
        android:background="@drawable/login_shape"
        android:gravity="center_vertical"
        android:padding="@dimen/_15px_in720p" >

        <ImageView
            android:layout_width="@dimen/_30px_in720p"
            android:layout_height="@dimen/_30px_in720p"
            android:layout_marginRight="@dimen/_5px_in720p"
            android:src="@drawable/login_username" />

        <EditText
            android:id="@+id/register_phone_num_ed"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:hint="@string/register_phone_num"
            android:textColor="#333333"
            android:inputType="phone"
            android:textColorHint="#dedede"
            android:textSize="@dimen/_30px_in720p"
            />
    </LinearLayout>
