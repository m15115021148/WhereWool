    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_120px_in720p"
        android:background="@color/user_xian"
        android:gravity="center" >

        <EditText
   	        android:id="@+id/baseDate"
   	       android:layout_width="@dimen/_300px_in720p"
            android:layout_height="@dimen/_80px_in720p"
   	       android:background="@drawable/datebg"
   	        android:gravity="center_vertical"
   	        android:textColor="@color/user_login_edit_font_color"
   	        android:hint="请选择月份"
   	        android:paddingLeft="@dimen/_20px_in720p"
   	        android:paddingRight="@dimen/_10px_in720p"
   	        android:paddingTop="@dimen/_10px_in720p"
   	        android:paddingBottom="@dimen/_10px_in720p"
   	        android:textSize="@dimen/_30px_in720p" />
    </LinearLayout>

    <com.baoyz.swipemenulistview.SwipeMenuListView
        android:id="@+id/location_detailed_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        
     />

</LinearLayout>