<?xml version="1.0" encoding="utf-8"?>
<!-- 功能页面 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white"
        android:padding="@dimen/_15px_in720p" >

        <LinearLayout
            android:id="@+id/base_back_lay"
            android:layout_width="@dimen/_80px_in720p"
            android:layout_height="@dimen/_80px_in720p"
            android:layout_centerVertical="true"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/back_tv"
                android:layout_width="@dimen/_32px_in720p"
                android:layout_height="@dimen/_32px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:background="@drawable/my_03" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="使用帮助"
            android:textColor="@color/ghei"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>

    <WebView
        android:id="@+id/gongneng_webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/gbei"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/_20px_in720p"
        android:paddingRight="@dimen/_1px_in720p"
        android:text="热点问题"
        android:textSize="@dimen/_30px_in720p" />

    <RelativeLayout
            android:id="@+id/wt01"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
        
            android:layout_width="match_parent"
            android:layout_height="@dimen/_100px_in720p"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/_20px_in720p"
            android:paddingRight="@dimen/_1px_in720p"
            android:text="如何选择/切换联系人，以及快捷添加联系人"
            android:textSize="@dimen/_26px_in720p" />

        <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/my_34"
             />
    </RelativeLayout>
	<View 
	     android:layout_width="match_parent"
        android:layout_height="@dimen/_2px_in720p"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:background="@color/gbei"
	    />
	  <RelativeLayout
	      android:id="@+id/wt02"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_100px_in720p"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/_20px_in720p"
            android:paddingRight="@dimen/_1px_in720p"
            android:text="对联系人的定位/追踪记录如何清除"
            android:textSize="@dimen/_26px_in720p" />

        <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/my_34"
             />
    </RelativeLayout>
	<View 
	     android:layout_width="match_parent"
        android:layout_height="@dimen/_2px_in720p"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:background="@color/gbei"
	    />
