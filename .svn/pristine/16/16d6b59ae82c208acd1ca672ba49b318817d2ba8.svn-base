            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/my_34"
          />
    </RelativeLayout>
	<View 
	     android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/gbei"
	    />
</LinearLayout>