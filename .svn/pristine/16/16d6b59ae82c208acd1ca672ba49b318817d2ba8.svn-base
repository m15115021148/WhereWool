        android:layout_height="@dimen/_2px_in720p"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:background="@color/gbei"
	    />
	 <RelativeLayout
	     android:id="@+id/wt06"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:layout_width="match_parent"
            android:layout_height="@dimen/_100px_in720p"
            android:gravity="center_vertical"
            android:paddingLeft="@dimen/_20px_in720p"
            android:paddingRight="@dimen/_1px_in720p"
            android:text="如何购买/变更套餐？"
