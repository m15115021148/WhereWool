<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >
    
 <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white"
        android:padding="@dimen/_15px_in720p" >

        <LinearLayout
            android:id="@+id/base_back_lay"
            android:layout_width="@dimen/_80px_in720p"
            android:layout_height="@dimen/_80px_in720p"
            android:layout_centerVertical="true"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/back_tv"
                android:layout_width="@dimen/_32px_in720p"
                android:layout_height="@dimen/_32px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:background="@drawable/my_03" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="问题帮助"
            android:textColor="@color/ghei"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>
    
 <View 
     android:layout_width="match_parent"
            android:layout_height="@dimen/_30px_in720p"
            android:background="@color/gbei"
     />
 		<TextView 
 		    android:id="@+id/wt_title"
 		     android:layout_width="match_parent"
            android:layout_height="@dimen/_100px_in720p"
            android:paddingLeft="@dimen/_20px_in720p"
            android:textSize="@dimen/_32px_in720p"
            android:paddingRight="@dimen/_20px_in720p"
			android:gravity="center_vertical"            
            android:text="如何选择/切换联系人，以及快捷添加联系人"
 		    />
    <TextView 
         android:id="@+id/wt_answer"
 		     android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingLeft="@dimen/_20px_in720p"
            android:textSize="@dimen/_26px_in720p"
            android:paddingRight="@dimen/_20px_in720p"
            android:lineSpacingMultiplier="1.5"
            android:text="答：在首页想要定位一个联系人，先得选择，点击地图右上角的常"
 		    />
    
     <TextView 
         android:id="@+id/wt_img"
 		     android:layout_width="@dimen/_680px_in720p"
            android:layout_height="@dimen/_430px_in720p"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/wt1_03"
            android:layout_marginTop="@dimen/_40px_in720p"
 		    />
</LinearLayout>
