<?xml version="1.0" encoding="utf-8"?>
<!-- 好友详细信息 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     android:background="@color/user_login_bottom_color"
    android:orientation="vertical" >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white"
        android:padding="@dimen/_15px_in720p" >

        <LinearLayout
            android:id="@+id/base_back_lay"
            android:layout_width="@dimen/_50px_in720p"
            android:layout_height="@dimen/_40px_in720p"
            android:layout_centerVertical="true"
            android:gravity="center_vertical" >

            <TextView
                android:id="@+id/back_tv"
                android:layout_width="@dimen/_32px_in720p"
                android:layout_height="@dimen/_32px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:background="@drawable/my_03" />
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="号码详情"
            android:textColor="@color/black"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>

 <TextView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="@dimen/_20px_in720p"
        android:background="@color/user_login_bottom_color" />


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_140px_in720p"
        android:background="@color/white" >

        <ImageView
            android:id="@+id/user_head"
            android:layout_width="@dimen/_88px_in720p"
            android:layout_height="@dimen/_88px_in720p"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:background="@drawable/detailed_imga" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_toRightOf="@id/user_head"
            android:gravity="center_vertical"
            android:orientation="vertical" >

               <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal" >
            <TextView
                android:id="@+id/user_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_20px_in720p"
                android:textColor="@color/lan"
                android:layout_marginBottom="@dimen/_5px_in720p"
                android:textSize="@dimen/_36px_in720p" />
            <TextView 
                 android:id="@+id/user_name_change_tv"
                android:layout_width="@dimen/_32px_in720p"
                android:layout_height="@dimen/_32px_in720p"
                android:layout_marginLeft="@dimen/_20px_in720p"
                android:layout_marginBottom="@dimen/_5px_in720p"
                android:background="@drawable/xiugaibeizhu"
                />
</LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginTop="@dimen/_5px_in720p"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="@dimen/_30px_in720p"
                    android:layout_height="@dimen/_30px_in720p"
                    android:layout_marginLeft="@dimen/_20px_in720p"
                    android:layout_marginRight="@dimen/_10px_in720p"
                    android:background="@drawable/user_phone" />

                <TextView
                    android:id="@+id/user_phone_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:text="15787856897"
                    android:textColor="#333333"
                    android:textSize="@dimen/_24px_in720p" />
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    
    <TextView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="@dimen/_20px_in720p"
        android:background="@color/user_login_bottom_color" />
    
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
              android:layout_marginLeft="@dimen/_20px_in720p"
            android:text="当前套餐："
            android:textSize="@dimen/_32px_in720p" />

        <TextView
            android:id="@+id/now_taocan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_20px_in720p"
             android:textSize="@dimen/_32px_in720p" 
            android:textColor="@color/lan" />

    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="10dp"
        android:background="@color/user_xian" />
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
              android:layout_marginLeft="@dimen/_20px_in720p"
            android:text="剩余次数："
            android:textSize="@dimen/_32px_in720p" />

        <TextView
            android:id="@+id/end_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:text="0"
             android:textSize="@dimen/_32px_in720p" 
            android:textColor="#333333" />

    </RelativeLayout>
    
    <TextView
        android:id="@+id/delete"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:layout_marginLeft="@dimen/_30px_in720p"
        android:layout_marginRight="@dimen/_30px_in720p"
        android:layout_marginTop="@dimen/_120px_in720p"
        android:background="@drawable/outbg"
        android:gravity="center"
        android:text="删除号码"
        android:textColor="#ffffff"
        android:textSize="@dimen/_35px_in720p" />
</LinearLayout>
