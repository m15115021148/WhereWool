<?xml version="1.0" encoding="utf-8"?>
<!-- 设置页面 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/user_login_bottom_color"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white"
        android:padding="@dimen/_15px_in720p" >
       <LinearLayout
            android:id="@+id/base_back_lay"
            android:layout_width="@dimen/_80px_in720p"
            android:layout_height="@dimen/_80px_in720p"
            android:gravity="center_vertical"
            android:layout_centerVertical="true">

            <TextView
                android:id="@+id/back_tv"
                android:layout_width="@dimen/_32px_in720p"
                android:layout_height="@dimen/_32px_in720p"
                android:layout_marginLeft="@dimen/_10px_in720p"
                android:background="@drawable/my_03" />
        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/shezhi_title"
            android:textColor="@color/ghei"
            android:textSize="@dimen/_36px_in720p" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1px_in720p"
        android:layout_marginTop="@dimen/_20px_in720p"
        android:background="@color/user_xian" />

    




    <RelativeLayout
        android:id="@+id/user_safe_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white" >

        <TextView
            
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:text="修改密码"
            android:gravity="center_vertical"
            android:textColor="#333333"
            android:textSize="@dimen/_30px_in720p" />
 <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_34" />

    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1px_in720p"
        android:layout_marginBottom="@dimen/_20px_in720p"
        android:background="@color/user_xian" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/user_xian" />
     <RelativeLayout
        android:id="@+id/user_gongneng_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white" >

        <TextView
            
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:drawablePadding="3dp"
            android:text="@string/shezhi_gongneng"
            android:gravity="center_vertical"
            android:textColor="#333333"
            android:textSize="@dimen/_30px_in720p"
           />
            <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_34" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1px_in720p"
        android:layout_marginBottom="@dimen/_20px_in720p"
        android:background="@color/user_xian" />
 
    <RelativeLayout
        android:id="@+id/yijian_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_100px_in720p"
        android:background="@color/white" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:drawablePadding="3dp"
            android:text="意见与反馈"
            android:gravity="center_vertical"
            android:textColor="#333333"
            android:textSize="@dimen/_30px_in720p" />

         <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_34" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/user_xian" />
    

    <RelativeLayout
        android:id="@+id/user_guanyu_lay"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@color/white" >

        <TextView
            
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_20px_in720p"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:drawablePadding="3dp"
            android:text="@string/shezhi_about"
            android:gravity="center_vertical"
            android:textColor="#333333"
            android:textSize="@dimen/_30px_in720p" />
          <ImageView
            android:layout_width="@dimen/_35px_in720p"
            android:layout_height="@dimen/_35px_in720p"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/_20px_in720p"
            android:background="@drawable/my_34" />
    </RelativeLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginBottom="5dp"
        android:background="@color/user_xian" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/user_xian" />

    <TextView
        android:id="@+id/user_out_tv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_80px_in720p"
        android:background="@drawable/outbg"
        android:gravity="center"
        android:layout_marginTop="@dimen/_60px_in720p"
        android:layout_marginLeft="@dimen/_20px_in720p"
        android:layout_marginRight="@dimen/_20px_in720p"
        android:text="@string/user_aqtc" 
        android:textColor="#ffffff"
            android:textSize="@dimen/_30px_in720p"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/user_xian" />

</LinearLayout>