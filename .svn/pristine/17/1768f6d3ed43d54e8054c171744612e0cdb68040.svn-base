                    <ImageView
                        android:id="@+id/index_dzwl_draw"
                        android:layout_width="@dimen/_114px_in720p"
                        android:layout_height="@dimen/_114px_in720p"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/_20px_in720p"
                        android:src="@drawable/index_dzwl_draw"
                        tools:ignore="ContentDescription" />
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/dingwei"
                android:layout_width="@dimen/_75px_in720p"
                android:layout_height="@dimen/_75px_in720p"
                android:layout_marginLeft="@dimen/_20px_in720p"
                android:layout_marginBottom="@dimen/_60px_in720p"
                android:layout_alignParentBottom="true"
                android:background="@drawable/bt33_06" />
            <RelativeLayout
                android:id="@+id/radiobutton_index_zhuizong"
                android:layout_width="@dimen/_75px_in720p"
                android:layout_height="@dimen/_75px_in720p"
                android:layout_marginRight="@dimen/_20px_in720p"
                android:layout_marginBottom="@dimen/_60px_in720p"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:background="@drawable/bt33_09" />
            <RelativeLayout
                 android:id="@+id/radiobutton_index_dingwei"
                android:layout_width="@dimen/_248px_in720p"
                android:layout_height="@dimen/_102px_in720p"
                android:layout_marginRight="@dimen/_20px_in720p"
                android:layout_marginBottom="@dimen/_48px_in720p"
                android:layout_alignParentBottom="true"
               android:layout_centerInParent="true"
                android:background="@drawable/bt33_03" />
            
          
            
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/index_friend_list"
        android:layout_width="@dimen/_400px_in720p"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layout_gravity="right"
        android:orientation="vertical" >

        <LinearLayout
             android:id="@+id/add_friend"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_180px_in720p"
            android:gravity="center_vertical" 
            android:orientation="horizontal">

            <TextView
                android:layout_width="@dimen/_70px_in720p"
                android:layout_height="@dimen/_70px_in720p"
                android:background="@drawable/add"
                android:layout_marginLeft="@dimen/_40px_in720p"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="添加号码"
                android:textColor="#333333"
                 android:layout_marginLeft="@dimen/_40px_in720p"
                android:textSize="@dimen/_30px_in720p" />
        </LinearLayout>
     <TextView
                android:layout_width="match_parent"
                android:layout_height="@dimen/_80px_in720p"
                android:paddingLeft="@dimen/_20px_in720p"
                android:paddingRight="@dimen/_20px_in720p"
                android:text="常用联系人"
                android:textColor="#666666"
                 android:gravity="center_vertical" 
                android:background="@color/user_login_bottom_color" />
       
 <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <ListView
                android:id="@+id/popupwinow_listview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="@null" >
            </ListView>

            
        </LinearLayout>
        
    </LinearLayout>

</android.support.v4.widget.DrawerLayout>